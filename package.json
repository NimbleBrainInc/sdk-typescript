{
  "name": "@nimblebrain/sdk",
  "version": "0.1.1",
  "description": "Official TypeScript SDK for NimbleBrain Studio API - AI agents, playbooks, and streaming conversations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.gen.js",
      "types": "./dist/client.gen.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "generate": "openapi-ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "npm run build"
  },
  "keywords": [
    "nimblebrain",
    "sdk",
    "api",
    "ai",
    "agents",
    "playbooks",
    "mcp",
    "streaming",
    "sse",
    "typescript"
  ],
  "author": "NimbleBrain <support@nimblebrain.ai>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NimbleBrainInc/sdk-typescript.git"
  },
  "homepage": "https://www.nimblebrain.ai",
  "bugs": {
    "url": "https://github.com/NimbleBrainInc/sdk-typescript/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.88.1",
    "typescript": "^5.6.0"
  }
}
